.div {
  font-size: 30rpx;
  padding: 40rpx 40rpx 100rpx 40rpx;
}

.container {
  display: flex;
  flex-direction: column;
  padding: 40rpx 0;
} 
  
.icon-box {
  margin: 20rpx 100rpx;
  border-radius: 30rpx;
}
.icon-box>image{
  width:500rpx;
  height:80rpx;
}


.fault-icon-text {
  font-size: 26rpx;
  font-weight: bold;
  vertical-align: middle;
}

/*上传图片  */

.upload view:nth-of-type(1) {
  margin: 20rpx 0 20rpx 0;
}
.imgUpload{
  margin:0 10rpx;
  width:160rpx; 
  height:160rpx;
}
/*checkbox  */

.checkBox {
  width: 100%;
  border: 1rpx solid #999;
  margin: 24rpx 0;
}

.upload_icon {
  width: 140rpx;
  height: 140rpx;
  vertical-align: middle;
}

.check_box {
  float: left;
  display: flex;
  flex-direction: column;
  margin-right: 20rpx;
  margin-left: 30rpx;
}

.check_box2 {
  margin-right: 0;
}

.checkbox {
  margin: 22rpx 5rpx;
  font-size: 30rpx;
  font-weight: 600;
}

.intro {
  box-sizing: border-box;
  border: 1px solid #999;
  width: 670rpx;
  height: 200rpx;
  font-size: 30rpx;
  color: #999;
  padding:20rpx;
}

.book-name {
  box-sizing: border-box;
  border: 1px solid #999;
  width: 670rpx;
  height: 80rpx;
  font-size: 30rpx;
  color: #999;
  padding: 20rpx;
  margin-bottom: 60rpx;
}

.submit {
  margin-top: 60rpx;
  color: #fff;
  background-color: #5082ce;
}

.submit_class{
  width: 360rpx;
  height: 88rpx;
  margin: auto auto;
}

.view_submit{
  display: flex;
  width: 100%;
  height: 200rpx;
}